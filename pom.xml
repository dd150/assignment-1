    <groupId>ming.assign1.251</groupId>
    <artifactId>NotePad</artifactId>
    <version>1.0-SNAPSHOT</version>

    <properties>
        <maven.compiler.release>1.8</maven.compiler.release>
    </properties>

    <build>
